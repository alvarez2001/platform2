<div class="home" id="home">

    {{>navbar}}


      <!-- Modal LOGIN -->
      <div
        class="modal fade"
        id="LoginPopup"
        data-bs-backdrop="static"
        data-bs-keyboard="false"
        tabindex="-1"
        aria-labelledby="LoginPopupLabel"
        aria-hidden="true"
      >
        <div class="modal-dialog modal-dialog-centered">
          <div class="modal-content">
            <div class="modal-body position-relative modal-nft">
              <p class="mb-0 text-end">
                <button
                  class="btn btn-closed p-0 m-0"
                  data-bs-dismiss="modal"
                  aria-label="Close"
                  type="button"
                >
                  <i class="fas fa-window-close"></i>
                </button>
              </p>
              <div class="px-md-5 px-2">
                <h2 class="modal-nft-title text-center m-0">Login</h2>
                <h3 class="text-center display-1 my-2">
                  <i class="fas fa-user-circle"></i>
                </h3>
                <h2 class="modal-nft-title text-center mb-4">
                  <img src="./imgs/logo.jpeg" alt="" width="200" height="50" />
                </h2>
                <div class="botones">
                  <a
                    href="./dashboard.html"
                    class="w-100 btn rojo font-bold-family25 btn-sesiones text-center"
                  >
                    <i class="fab fa-google-plus-g me-2"></i>
                    login with google
                  </a>
                  <a
                    href="./login.html"
                    class="w-100 btn azul-obscuro font-bold-family25 btn-sesiones text-center"
                  >
                    <i class="fas fa-envelope me-2"></i>
                    login with email
                  </a>
                </div>
                <p
                  data-bs-target="#RegisterPopup"
                  data-bs-toggle="modal"
                  data-bs-dismiss="modal"
                  class="d-block cursor-pointer mb-4 text-decoration-none text-end line-h-1 font-titulo font-15px font-weight-bold obscuro text-uppercase"
                >
                  already have an
                  <span class="d-block">account</span>
                </p>
              </div>
            </div>
          </div>
        </div>
      </div>

      <!-- Modal LOGIN -->

      <!-- Modal REGISTER -->
      <div
        class="modal fade"
        id="RegisterPopup"
        data-bs-backdrop="static"
        data-bs-keyboard="false"
        tabindex="-1"
        aria-labelledby="RegisterPopupLabel"
        aria-hidden="true"
      >
        <div class="modal-dialog modal-dialog-centered">
          <div class="modal-content">
            <div class="modal-body position-relative modal-nft">
              <p class="mb-0 text-end">
                <button
                  class="btn btn-closed p-0 m-0"
                  data-bs-dismiss="modal"
                  aria-label="Close"
                  type="button"
                >
                  <i class="fas fa-window-close"></i>
                </button>
              </p>
              <div class="px-md-5 px-2">
                <h2 class="modal-nft-title text-center m-0">register</h2>
                <h3 class="text-center display-1 my-2">
                  <i class="fas fa-user-circle"></i>
                </h3>
                <h2 class="modal-nft-title text-center mb-4">
                  <img src="./imgs/logo.jpeg" alt="" width="200" height="50" />
                </h2>
                <form action="" class="form-propio">
                  <div class="form-group">
                    <input
                      type="email"
                      class="form-control"
                      placeholder="EMAIL"
                    />
                  </div>
                  <div class="form-group">
                    <input
                      type="password"
                      class="form-control"
                      placeholder="PASSWORD"
                    />
                  </div>
                  <div class="form-group">
                    <input
                      type="password"
                      class="form-control"
                      placeholder="REPEAT PASSWORD"
                    />
                  </div>
                </form>
                <div class="text-center">
                  <button
                    data-bs-target="#LoginPopup"
                    data-bs-toggle="modal"
                    data-bs-dismiss="modal"
                    class="btn btn-obscuro btn-details mt-0 mb-4 rounded-0 px-4"
                  >
                    register
                  </button>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <!-- MODAL REGISTER -->


      <section class="hero d-flex justify-content-center align-items-center">
        <div class="hero-content container text-center">
